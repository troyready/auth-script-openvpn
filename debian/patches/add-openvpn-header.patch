Description: Include OpenVPN header file directory
 Compilation on Debian will fail if the header directory is not included.
Author: Troy Ready <troy@troyready.com>
Last-Update: 2017-10-26
--- a/Makefile
+++ b/Makefile
@@ -6,7 +6,7 @@ LDFLAGS += -fPIC -shared
 all: plugin
 
 plugin: auth_script.c
-	$(CC) $(CFLAGS) $(LDFLAGS) -o auth_script.so auth_script.c
+	$(CC) $(CFLAGS) $(LDFLAGS) -I/usr/include/openvpn -o auth_script.so auth_script.c
 
 clean:
 	rm -f *.o *.so
